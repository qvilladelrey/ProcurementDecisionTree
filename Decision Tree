        // Emoji mapping for procurement modes
        const procurementModeLinks = {
            "Competitive Bidding": "üèÜ",            
            "Limited Source Bidding": "üéØ",
            "Competitive Dialogue": "ü§ù",
            "Unsolicited Offer with Bid Matching": "‚≠ê",
            "Direct Contracting": "ü§ù",
